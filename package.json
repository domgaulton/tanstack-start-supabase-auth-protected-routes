{
  "name": "tanstack-start-supabase-auth",
  "private": true,
  "type": "module",
  "imports": {
    "#/*": "./src/*"
  },
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "typecheck": "tsc --noEmit",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset && npm run db:types && npm run db:seed",
    "db:seed": "tsx --env-file=.env supabase/seed.ts",
    "db:types": "supabase gen types typescript --local > src/types/database.types.ts && biome check --write src/types/database.types.ts",
    "db:migrate": "supabase migration up --local && npm run db:types",
    "prepare": "husky"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.97.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "@vercel/analytics": "^1.6.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.545.0",
    "nitro": "npm:nitro-nightly@latest",
    "radix-ui": "1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "3.5.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@tanstack/devtools-vite": "^0.3.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "husky": "9.1.7",
    "jsdom": "^27.0.0",
    "shadcn": "3.8.5",
    "supabase": "2.76.12",
    "tsx": "4.21.0",
    "tw-animate-css": "1.4.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  }
}
